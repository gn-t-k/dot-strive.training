{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev --host",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev ./build/client",
    "build-cf-types": "wrangler types",
    "migrations:gen": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "migrations:apply": "wrangler d1 migrations apply dot-strive-db --local",
    "migrations:drop": "drizzle-kit drop --config=./database/drizzle.config.ts"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.8.1",
    "@remix-run/cloudflare-pages": "2.8.1",
    "@remix-run/react": "2.8.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "drizzle-orm": "0.30.7",
    "isbot": "4.4.0",
    "lucide-react": "0.364.0",
    "miniflare": "3.20231030.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@cloudflare/workers-types": "4.20240403.0",
    "@remix-run/dev": "2.8.1",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "autoprefixer": "10.4.19",
    "drizzle-kit": "0.20.14",
    "node-fetch": "3.3.2",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.1.6",
    "vite": "5.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "wrangler": "3.44.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
