{
  "name": "dot-strive",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev --host",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev ./build/client",
    "build-cf-types": "wrangler types",
    "migrations:gen": "drizzle-kit generate:sqlite --config=./database/drizzle.config.ts",
    "migrations:apply": "wrangler d1 migrations apply dot-strive-db --local",
    "migrations:drop": "drizzle-kit drop --config=./database/drizzle.config.ts"
  },
  "dependencies": {
    "@conform-to/react": "1.1.0",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-select": "2.0.0",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-visually-hidden": "1.0.3",
    "@remix-run/cloudflare": "2.8.1",
    "@remix-run/cloudflare-pages": "2.8.1",
    "@remix-run/react": "2.8.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "conform-to-valibot": "1.6.0",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.30.8",
    "isbot": "5.1.4",
    "lucide-react": "0.368.0",
    "miniflare": "3.20240405.1",
    "react": "18.2.0",
    "react-day-picker": "8.10.0",
    "react-dom": "18.2.0",
    "remix-auth": "3.6.0",
    "remix-auth-google": "2.0.0",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7",
    "valibot": "0.30.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@cloudflare/workers-types": "4.20240405.0",
    "@remix-run/dev": "2.8.1",
    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "drizzle-kit": "0.20.14",
    "node-fetch": "3.3.2",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.8",
    "vite-tsconfig-paths": "4.3.2",
    "wrangler": "3.50.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
